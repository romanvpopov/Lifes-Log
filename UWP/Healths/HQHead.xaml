<UserControl
    x:Class="LL.Healths.HQHead"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml">
    <Border x:Name="BL"
            Margin="2"
            BorderBrush="{ThemeResource ButtonBorderThemeBrush}"
            BorderThickness="1"
            CornerRadius="2"
            Tapped="BL_Tapped"
            PointerEntered="BL_PointerEntered"
            PointerExited="BL_PointerExited">
        <TextBlock x:Name="TX"/>
    </Border>
</UserControl>
